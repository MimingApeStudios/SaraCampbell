<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mimingape.saracampbell" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SaraCampbell</name>
    <description>Sara Campbell is built on a philosophy of giving back to the community, while creating and delivering
not only a unique and beautiful product, but a transformational shopping experience in our boutiques.
 </description>
    <author email="mimingapestudios@gmail.com" href="http://mimingape.com">Miming Ape Studios</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
